<resources>
    <!-- Base application theme. -->
    <style name="Theme.DicodingStoryApp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dicoding</item>
        <item name="colorPrimaryVariant">@color/dicoding</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dicoding</item>
        <item name="colorSecondaryVariant">@color/white</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="color">@color/white</item>
    </style>
    <style name="customImageView8Percent" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">8%</item>
    </style>
    <style name="customImageView4Percent" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">4%</item>
    </style>
</resources>